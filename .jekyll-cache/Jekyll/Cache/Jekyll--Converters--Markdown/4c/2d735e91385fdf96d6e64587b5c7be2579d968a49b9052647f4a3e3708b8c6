    <span class="kd">private</span> <span class="kd">static</span> <span class="kt">int</span> <span class="n">numInstences</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span>    <span class="c1">// é™æ€å˜é‡</span>

    <span class="n">prvate</span> <span class="kd">static</span> <span class="kt">void</span> <span class="nf">addInstence</span><span class="o">(){</span>       <span class="c1">// é™æ€å‡½æ•°</span>
